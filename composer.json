{
    "name": "OlaHub Designers",
    "description": "OlaHub Designers application.",
    "keywords": ["OlaHub","celebration", "designers"],
    "license": "OlaHub LLC",
    "type": "project",
    "require": {
        "php": ">=7.1.3",
        "barryvdh/laravel-cors": "^0.11.0",
        "illuminate/mail": "^5.8",
        "jenssegers/mongodb": "^3.5",
        "laravel/lumen-framework": "5.8.*",
        "league/fractal": "^0.17.0",
        "vlucas/phpdotenv": "^3.3"
    },
    "require-dev": {
        "fzaninotto/faker": "^1.4",
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "classmap": [
            "Common",
            "Modules"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
